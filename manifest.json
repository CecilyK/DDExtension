{
	"name":"David Dobrik",
	"version":"0.0.1",
	"manifest_version":2,
	"description":"Plays David Dobrik's intro when you hover over the name 'David Dobrik'",
	"icons":{
		"48":"icon.png"
	 		},
	"content_scripts":[
		{
			"matches":["<all_urls>"],
			"js":["index.js"],
			"css":["dd.css"]
		}

	],
	"browser_action":{
		"default_popup":"dd.html",
		"default_icon":{
			"16":"icon.png"
		}
    },
    "web_accessible_resources": [
        "song.mp3"
    ]
}